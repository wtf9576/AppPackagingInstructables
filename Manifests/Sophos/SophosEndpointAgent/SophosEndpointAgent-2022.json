{
  "id": "sophos.sophosendpointagent.2022",
  "name": "Sophos Endpoint Agent",
  "version": "2022",
  "publisher": "Sophos",
  "description": "Sophos Endpoint Agent 2022 - enterprise packaging template.",
  "source": {
    "type": "local",
    "relativePath": "payload\\\\setup.exe"
  },
  "installer": {
    "type": "exe",
    "relativePath": "payload\\\\setup.exe",
    "silentArgs": "/quiet",
    "uninstallCmd": "UNKNOWN_SOPHOS_UNINSTALL"
  },
