{
  "id": "autodesk.civil3d.2018",
  "name": "Autodesk Civil 3D",
  "version": "2018",
  "publisher": "Autodesk",
  "description": "Autodesk Civil 3D 2018 - packaging instructable template.",
  "source": {
    "type": "local",
    "relativePath": "Img\\Setup.exe"
  },
  "installer": {
    "type": "exe",
    "relativePath": "Img\\Setup.exe",
    "silentArgs": "/W /q /I Img\\Civil_3D_2018.ini /language en-us",
    "uninstallCmd": "UNKNOWN_C3D2018_UNINSTALL"
  },
  "detection": {
    "type": "uninstallReg",
    "keyPath": "HKLM\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\Uninstall",
    "valueName": "DisplayName",
    "operator": "contains",
    "expectedValue": "Autodesk Civil 3D 2018"
  },
  "requirements": {
    "minOSVersion": "10.0.14393",
    "architecture": "x64"
  },
  "intuneHints": {
    "displayName": "Autodesk Civil 3D 2018",
    "category": "Engineering",
    "restartBehavior": "suppress",
    "notifications": "showAll"
  },
  "variables": {},
  "caveats": [
    "Uses legacy AutoCAD-based setup with INI response file.",
    "Ensure Img folder structure is preserved in Intune source content."
  ],
  "review": {
    "reviewed": false,
    "reviewedBy": null,
    "reviewedDate": null
  }
}