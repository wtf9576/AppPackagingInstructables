{
  "id": "autodesk.autocadmap3d.2023",
  "name": "AutoCAD Map 3D",
  "version": "2023",
  "publisher": "Autodesk",
  "description": "AutoCAD Map 3D 2023 - packaging instructable template.",
  "source": {
    "type": "local",
    "relativePath": "Installer.exe"
  },
  "installer": {
    "type": "exe",
    "relativePath": "Installer.exe",
    "silentArgs": "-i deploy --offline_mode --ui_mode basic -o .\\Collection.xml --installer_version \"1.31.0.546\"",
    "uninstallCmd": "Installer.exe -i uninstall -q --manifest .\\MAP_2023_en-US\\setup.xml --extension_manifest .\\MAP_2023_en-US\\setup_ext.xml"
  },
  "detection": {
    "type": "uninstallReg",
    "keyPath": "HKLM\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\Uninstall",
    "valueName": "DisplayName",
    "operator": "contains",
    "expectedValue": "AutoCAD Map 3D 2023"
  },
  "requirements": {
    "minOSVersion": "10.0.14393",
    "architecture": "x64"
  },
  "intuneHints": {
    "displayName": "AutoCAD Map 3D 2023",
    "category": "Engineering",
    "restartBehavior": "suppress",
    "notifications": "showAll"
  },
  "variables": {},
  "caveats": [
    "MAP_2023_en-US folder and setup XML files must be present at package root.",
    "Uses ODIS-style deploy and local manifest-based uninstall."
  ],
  "review": {
    "reviewed": false,
    "reviewedBy": null,
    "reviewedDate": null
  }
}